    Private Function SetParameter(ByRef Sqlcommand As SqlClient.SqlCommand, ByVal strMode As String) As Boolean
        Try
            Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@InterView_ID", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mInterView_ID, SqlDbType.Int)
            Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@Applicant_ID", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mApplicant_ID, SqlDbType.Int)
            Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@Interviewer_ID", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mInterviewer_ID, SqlDbType.Int)
            Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@DefaultEvalType", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mDefaultEvalType, SqlDbType.Int)
            Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@Garde", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mGarde, SqlDbType.Int)
            Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@Evaluation", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mEvaluation, SqlDbType.Int)
            Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@PowerPCT", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mPowerPCT, SqlDbType.Int)
            If (strMode.Trim.ToUpper = "SAVE") Then
                Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@RegUserID", SqlDbType.Int)).Value = mDataHandler.DataValue_In(Me.mDataBaseUserRelatedID, SqlDbType.Int, True)
                Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@RegComputerID", SqlDbType.Int)).Value = mDataHandler.DataValue_In(mRegComputerID, SqlDbType.Int, True)
                Sqlcommand.Parameters.Add(New SqlClient.SqlParameter("@CompanyID", SqlDbType.Int)).Value = mDataHandler.DataValue_In(Me.MainCompanyID, SqlDbType.Int, True)
            End If
        Catch ex As Exception
            mPage.Session.Add("ErrorValue", ex)
            mErrorHandler.RecordExceptions_DataBase("", ex, Err.Number, mDataBaseUserID, Venus.Shared.ErrorsHandler.eRecordingType.System_ErrorsLog)
            mPage.Response.Redirect("ErrorPage.aspx")
        End Try
    End Function

