        Try
            With Ds.Tables(0).Rows(0)
                mID = mDataHandler.DataValue_Out(.Item("ID"), SqlDbType.Int, True)
                mGradeID = mDataHandler.DataValue_Out(.Item("GradeID"), SqlDbType.Int, True)
                mVacationTypeID = mDataHandler.DataValue_Out(.Item("VacationTypeID"), SqlDbType.Int, True)
                mDurationDays = mDataHandler.DataValue_Out(.Item("DurationDays"), SqlDbType.TinyInt)
                mRequiredWorkingMonths = mDataHandler.DataValue_Out(.Item("RequiredWorkingMonths"), SqlDbType.TinyInt)
